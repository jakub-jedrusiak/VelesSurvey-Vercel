{
    "builds": [
      {
        "src": "VelesSurvey/wsgi.py",
        "use": "@vercel/python",
        "runtime": "python3.11",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "VelesSurvey/wsgi.py"
      }
    ]
}
